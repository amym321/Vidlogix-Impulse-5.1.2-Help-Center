<div class="faq-section">
  <button style="padding: 11px 0 30px;" type="button" class="sub-block collapsible-trigger collapsible-trigger--inline collapsible--auto-height text-left"  aria-controls="HC-{{ block_id }}-{{sub_block}}">
    {%- render 'collapsible-icons-alt', no_circle: true -%}
    <h3 style="margin-top: 0; margin-bottom: 0;"><span>{{ sub_heading}}</span></h3>
  </button>
  <div id="HC-{{ block_id }}-{{ sub_block }}" class="collapsible-content collapsible-content--all" itemscope itemprop="acceptedAnswer" itemtype="https://schema.org/Answer">
    <div class="collapsible-content__inner collapsible-content__inner--faq rte" style="padding-top:0;" itemprop="text">
      {{ sub_text }}
    </div>
  </div>
</div>