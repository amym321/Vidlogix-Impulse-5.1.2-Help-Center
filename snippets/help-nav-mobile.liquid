<style>
@media (min-width:1024px){
  .help-center-nav {
    display:none;
  }
}

/* sticky header */
.help-center-nav .sticky {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 5;
}

.help-center-nav .hc-nav-wrapper {
  background: #171717;
  overflow-x: scroll;
  overflow-y: hidden;
  display: flex;
}

.help-center-nav .hc-nav-list {
  margin: 0 auto;
  padding: 0 20px;
  white-space: nowrap;
  display: flex;
  -webkit-box-pack: center;
  justify-content: center; 
  list-style: none;
  transition: opacity .5s cubic-bezier(.25,.46,.45,.94);
  margin-block-start: 4px;
  margin-block-end: 1em;
  margin-inline-start: 0px;
  margin-inline-end: 0px;
}

.help-center-nav .hc-nav-list-item {
  box-sizing: border-box;
  height: 50px;
  padding: 2px 16px 0;
  display: flex;
  -webkit-box-align: center; align-items: center;
  transition:padding .2s cubic-bezier(.25,.46,.45,.94);
  text-align: -webkit-match-parent;
}

.help-center-nav .hc-nav-list-title  {
  margin:unset;
  font-size: 13px;
  color: #fff;
  outline: 0;
  display: inline-block;
  padding: 0;
  box-sizing: border-box;
  overflow: hidden;
  position: relative;
  text-transform: uppercase;
  background-color: transparent;
  cursor: pointer;
}

.help-center-nav .hc-subnav-wrapper {
  height: auto !important;
  visibility: hidden;
  position: absolute;
  background: #808080;
  top: 55px;
  left: 0;
  width: 100%;
  z-index: 2;
  overflow-x: scroll;
  overflow-y: hidden;
  display:flex !important;
}


.help-center-nav  .sub-nav-list {
  margin: 0 auto;
